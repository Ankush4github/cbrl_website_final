@article{CHAUDHURY2013439,
title = {Mitigation of endometriosis using regenerative cerium oxide nanoparticles},
journal = {Nanomedicine: Nanotechnology, Biology and Medicine},
volume = {9},
number = {3},
pages = {439-448},
year = {2013},
issn = {1549-9634},
doi = {https://doi.org/10.1016/j.nano.2012.08.001},
url = {https://www.sciencedirect.com/science/article/pii/S154996341200514X},
author = {Koel Chaudhury and Narendra {Babu K} and Abhay K. Singh and Soumen Das and Amit Kumar and Sudipta Seal},
keywords = {Cerium oxide nanoparticles, Antioxidant, Endometriosis, Anti-angiogenesis},
abstract = {Cerium oxide nanoparticles (nanoceria) have recently received attention from the scientific community due to their unique free radicals (specially superoxide radical and hydrogen peroxide) scavenging property in biological system, both in vitro and in vivo. It is suggested that free radicals play an important role in the pathogenesis of endometriosis. In this study we have shown that nanoceria mitigate the endometrial lesions induced in mice model by decreasing oxidative stress and inhibiting angiogenesis. Moreover, nanoceria were also observed to protect endometriosis-related adverse effects on the oocytes, which is critical for successful pregnancy. Summarizing, nanoceria have shown promising efficacy against endometriosis related pathogenesis.
From the Clinical Editor
Free radicals have been implemented in the pathogenesis of endometriosis. In a murine model the authors demonstrated successful treatment of endometriosis with nanoceria, and protection of endometriosis-related adverse effects on the oocytes, paving the way to potential clinical translational applications in the future.}
}

