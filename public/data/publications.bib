@article{Ghosh2021,
  author    = {Nilanjana Ghosh and Priyanka Choudhury and Mamata Joshi and Parthasarathi Bhattacharyya and Sushmita Roychowdhury and Rintu Banerjee and Koel Chaudhury},
  title     = {Global metabolome profiling of exhaled breath condensates in male smokers with asthma COPD overlap and prediction of the disease},
  journal   = {Scientific Reports},
